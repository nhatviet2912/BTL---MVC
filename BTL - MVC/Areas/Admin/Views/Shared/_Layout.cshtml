@{

    var user = (BTL___MVC.Database.TaiKhoan)System.Web.HttpContext.Current.Session["admin"];
    
}
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.title</title>

    <!-- Custom fonts for this template-->
    <link href="~/Assets/admin/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link href="~/Assets/admin/css/sb-admin-2.min.css?v=3.4.1" rel="stylesheet" />
    <link href="~/Assets/admin/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="#">
                <div class="sidebar-brand-icon " style="max-width: 50px;">
                    <img src="~/Image/@user.Anh" style="width: 100%; height: 100%; border-radius: 50%;" />
                </div>
                <div class="sidebar-brand-text mx-3">
                    @user.Tentk
                </div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <!-- Nav Item - Dashboard -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/TrangChu/TrangChu">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Trang quản trị</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">



            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/LoaiSanPhams/Index">
                    <i class="fas fa-fw fa-cog"></i>
                    <span>Quản lý loại sản phẩm</span>
                </a>

            </li>

            <!-- Nav Item - Utilities Collapse Menu -->
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/SanPhams/Index">
                    <i class="fas fa-fw fa-wrench"></i>
                    <span>Quản lý sản phẩm</span>
                </a>

            </li>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/NCCS/Index">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý nhà cung cấp</span>
                </a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <li class="nav-item">
                <a class="nav-link" href="/Admin/DonHangs/Index">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý đơn hàng</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/DonHangs/DonHangDaBan">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý đơn hàng đã bán</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/DonHangs/Chitiet">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý chi tiết hóa đơn</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/HoaDonNhaps/Index">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý hóa đơn nhập</span>
                </a>
            </li>


            <!-- Divider -->
            <hr class="sidebar-divider">


            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item ">
                <a class="nav-link" href="/Admin/KhachHangs/Index">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>Quản lý khách hàng</span>
                </a>

            </li>

            <!-- Nav Item - Charts -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/NhanViens/Index">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Quản lý nhân viên</span>
                </a>
            </li>

            <hr class="sidebar-divider">


            <!-- Nav Item - Tables -->
            <li class="nav-item">
                <a class="nav-link" href="/Admin/TaiKhoans/Index">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Quản lý Tài khoản</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link" href="/Admin/Contacts/Index">
